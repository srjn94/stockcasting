{
    "model_version": "mlp",
    
    "learning_rate": 1e-3,
    "window_size": 10,
    "batch_size": 50,
    "num_epochs": 10,
    
    "dense_blocks": [
        {
            "dense": {"units": 2, "kernel_initializer": "${tf.contrib.layers.xavier_initializer()}"},
            "bn": {"momentum": 0.99, "gamma_initializer": "${tf.contrib.layers.xavier_initializer()}"},
            "dropout": {"rate": 0.5}
        },
        {
            "dense": {"units": 1, "kernel_initializer": "${tf.contrib.layers.xavier_initializer()}"},
            "bn": {"momentum": 0.99, "gamma_initializer": "${tf.contrib.layers.xavier_initializer()}"},
            "dropout": {"rate": 0.5}
        },
        {
            "dense": {"units": 6, "kernel_initializer": "${tf.contrib.layers.xavier_initializer()}"},
            "bn": {"momentum": 0.99, "gamma_initializer": "${tf.contrib.layers.xavier_initializer()}"},
            "dropout": {"rate": 0.5}
        }
    ],
    "output_block": {
        "logits": {"units": 3}
    },

    "save_summary_steps": 100
}
